{{#uni-input-container isChecked=checked}}
  <div class="uni-radio-button">
    <input
      type="radio"
      value={{value}}
      checked={{checked}}
      name={{name}}
      id={{i '${id}-radio' id=elementId}}>

    <label for={{i '${id}-radio' id=elementId}}>{{label}}</label>
  </div>
{{/uni-input-container}}
